data = dataframe(parts=["noncoding", "coding"], nchar=[200,200], ntaxa=16);
coding = part(frame=data, name="coding");
noncoding = part(frame=data, name="noncoding");
Θ ~ LogNormal(meanlog=-3.0, sdlog=1.0);
ψ ~ Coalescent(theta=Θ, n=ntaxa(taxa=data));
π ~ Dirichlet(conc=[2.0,2.0,2.0,2.0]);
Q = f81(freq=π);
D1 ~ PhyloCTMC(frame=coding, Q=Q, tree=ψ);
D2 ~ PhyloCTMC(frame=noncoding, Q=Q, tree=ψ);